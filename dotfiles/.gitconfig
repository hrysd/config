[user]
  name  = Hiroshi Yoshida
  email = hrysd22@gmail.com
[core]
  editor = vim
  excludesfile = ~/.gitignore
[color]
  ui = auto
[alias]
  ba = branch -a
  br = branch
  cb = checkout -b
  ch = checkout
  ci = commit
  cv = commit -av
  dc = diff --cached
  df = diff
  g  = grep -n
  l  = log --graph --stat
  rt = remote
  rv = remote -v
  st = status
[pager]
  log  = diff-highlight | less
  show = diff-highlight | less
  diff = diff-highlight | less
[credential]
  helper = store
